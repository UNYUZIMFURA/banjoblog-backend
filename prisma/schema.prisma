generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book {
  id String @id @default(uuid())
  name String
  author String 
  publisher String
  publicationYear Int
  subject String
}

model User {
  id       String    @id @default(uuid())
  firstName String 
  lastName String
  email    String    @unique
  password String
}